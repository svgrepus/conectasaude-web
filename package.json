    "update-all-configs": "node sync-supabase-config.js",
    "clean-all-configs": "node clean-all-configs.js",
    "reset-configs": "node clean-all-configs.js && node sync-supabase-config.js",
    "config:sync": "node sync-supabase-config.js",
    "config:validate": "node -e \"const {SUPABASE_MASTER_CONFIG} = require('./supabase.master.config.js'); console.log('âœ… Config OK:', SUPABASE_MASTER_CONFIG.PROJECT_REF)\""
